    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^3.0.5",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-build-notifier": "^1.1.1",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2",
    "write-file-webpack-plugin": "^4.5.1"
  }
